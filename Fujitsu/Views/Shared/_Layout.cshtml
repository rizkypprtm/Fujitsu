<!DOCTYPE html>
<html lang="en">
<head>
    <partial name="MainPage/_Meta" />
    <!-- PAGE TITLE HERE -->
    <title>[@ViewBag.Title] Fujitsu</title>
    <!-- FAVICONS ICON -->
    @* <link rel="shortcut icon" type="image/png" href="~/w3crm/images/favicon.png"> *@
    @RenderSection("style", false)
    <link href="~/packages/vendor/datatables/css/jquery.dataTables.min.css" rel="stylesheet">
    <link href="~/packages/vendor/swiper/css/swiper-bundle.min.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Material+Icons" rel="stylesheet">
    <link href="~/packages/vendor/bootstrap-select/dist/css/bootstrap-select.min.css" rel="stylesheet">
