    <!--Main bootstrap-->
    <script src="~/packages/vendor/global/global.min.js"></script>
    <script src="~/packages/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>

    <!--Data tables-->
    <script src="~/packages/vendor/jquery/jquery.min.js"></script>
    <script src="~/packages/vendor/datatables/js/jquery.dataTables.min.js"></script>
    
    <!--Main JS-->
    <script src="~/packages/vendor/metismenu/js/metisMenu.min.js"></script>
    <script src="~/packages/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="~/packages/js/custom.js"></script>
    <script src="~/packages/js/deznav-init.js"></script>
    <script src="~/packages/js/demo.js"></script>
    
    <script src="~/packages/vendor/toastr/js/toastr.min.js"></script>
    <script src="~/packages/vendor/sweetalert2/dist/sweetalert2.min.js"></script>

    @* <script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/2.0.0/FileSaver.min.js"></script> *@
